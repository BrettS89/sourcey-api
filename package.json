    "dotenv": "^16.0.1",
    "feathers-hooks-common": "^6.1.5",
    "feathers-mongoose": "^8.5.1",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb-core": "^3.2.7",
    "mongoose": "^6.5.1",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
